<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>EMP List</title>
</head>
<body>
    <a href="/admin">Back</a>
    <!-- <a href="/logout">Logout</a> -->
	
	<h3>All EMP List</h3>
	<table border="1">
		<tr>
			<th>EMP ID</th>
			<th>USERNAME</th>
			<th>PASSWORD</th>		
			<th>TYPE</th>
			<th>PHONE</th>
			<th>GENDER</th>
			<th>DESIGNATION</th>
			<th>ACTION</th>
		</tr>

		<% for(var i=0; i < userList.length; i++){ %>
		<tr>
			<td><%= userList[i].empid %></td>
			<td><%= userList[i].username %></td>
			<td><%= userList[i].password %></td>
			<td><%= userList[i].type %></td>
			<td><%= userList[i].phone %></td>
			<td><%= userList[i].gender %></td>
			<td><%= userList[i].designation %></td>
			<td><a href="/admin/Update/<%= userList[i].empid %>">UPDATE</a> | <a href="/admin/Delete/<%= userList[i].empid %>"> DELETE </a></td>
		</tr>
		<% } %>
    </table>
</body>
</html>